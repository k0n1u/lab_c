//Program do oblicznia pierwiastka kwadratowego liczby
#include <stdio.h>
#include <math.h>

int main()
{
  double a,p;

  printf("Podaj liczbe:\n");
  scanf("%lf",&a);

  if(a>=0)
    {
      p=sqrt(a);
      printf("Pierwiastek kwadratowy z %lf wynosi %lf\n",a,p);
    }
  else
    {
      printf("Podano liczbe ujemna.\n");
    }

  return 0;
}
