int suma(int a,int b,int c)
{
  return a+b+c;
}

int iloczyn(int a,int b, int c)
{
  return a*b*c;
}

int min(int a, int b, int c)
{
  int min=0;
  
  if(a>min)
    {
      min=a;
    }
  if(b>min)
    {
      min=b;
    }
  if(c>
min)
    {
      min=c;
    }

  return min;
}
