#include <stdio.h>

int suma(int,int,int);
int iloczyn(int,int,int);
int min(int,int,int);
int max(int,int,int);


int main()
{
  int a,b,c;

  printf("WprowadÅº trzy liczby caÅ‚kowite: ");
  scanf("%d",&a);
  scanf("%d",&b);
  scanf("%d",&c);
  
  printf("\nNajmniejsza: %d",min(a,b,c));
  
  //printf("\nA: %d\nB: %d\n C: %d\n",a,b,c);





